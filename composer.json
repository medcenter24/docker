{
  "name": "medcenter24/docker",
  "description": "Docker Configuration for the MedCenter24 application",
  "minimum-stability": "dev",
  "license": "GPL-2.0-only",
  "authors": [
    {
      "name": "Oleksandr Zagovorychev",
      "email": "zagovorichev@gmail.com"
    }
  ],
  "prefer-stable": true,
  "require": {
    "medcenter24/mc-core": "dev-develop",
    "medcenter24/mc-director-gui": "dev-develop",
    "medcenter24/mc-doctor-gui": "dev-develop"
  },
  "require-dev": {
    "ext-imagick": "*",
    "barryvdh/laravel-debugbar": "^3.6",
    "barryvdh/laravel-ide-helper": "^2.10",
    "filp/whoops": "^2.7.1",
    "facade/ignition": "^2.8.3",
    "fzaninotto/faker": "^1.9",
    "mockery/mockery": "^1.4",
    "nunomaduro/collision": "^5.5",
    "phpunit/phpunit": "^9.5",
    "roave/security-advisories": "dev-master"
  },
  "autoload-dev": {
    "psr-4": {
      "medcenter24\\mcCore\\Tests\\": "mcCore/Tests"
    }
  },
  "config": {
    "optimize-autoloader": true
  }
}
